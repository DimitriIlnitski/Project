    <a href="/products/create">Add product</a>
    <h1>List of products</h1>
    <table>
        <tr><th>Name</th><th>Volume</th><th>Material</th><th>Price</th></tr>
        {{#each products}}
            <tr><td>{{this.name}}</td><td>{{this.volume}}</td><td>{{this.material}}</td><td>{{this.price}}</td></tr>
            {{/each}}
    </table>
