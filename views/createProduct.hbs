<h1>Add product</h1>
<form action="postProduct" method="post">
    <label>Name</label>
    <input name="name"/><br><br>
    <label>Volume</label>
    <input name="volume" type="number" min='1' max='600'/><br><br>
    <label>Material</label>
    <input name="material"/><br><br>
    <label>Price</label>
    <input name="price"/><br><br>
    <input type="submit" value="Send" />
</form>
